                                                        <path d="M8.56066017,16.6819805 L10.6819805,14.5606602 C11.267767,13.9748737 12.2175144,13.9748737 12.8033009,14.5606602 L14.9246212,16.6819805 C15.5104076,17.267767 15.5104076,18.2175144 14.9246212,18.8033009 L12.8033009,20.9246212 C12.2175144,21.5104076 11.267767,21.5104076 10.6819805,20.9246212 L8.56066017,18.8033009 C7.97487373,18.2175144 7.97487373,17.267767 8.56066017,16.6819805 Z M8.56066017,4.68198052 L10.6819805,2.56066017 C11.267767,1.97487373 12.2175144,1.97487373 12.8033009,2.56066017 L14.9246212,4.68198052 C15.5104076,5.26776695 15.5104076,6.21751442 14.9246212,6.80330086 L12.8033009,8.9246212 C12.2175144,9.51040764 11.267767,9.51040764 10.6819805,8.9246212 L8.56066017,6.80330086 C7.97487373,6.21751442 7.97487373,5.26776695 8.56066017,4.68198052 Z" fill="#000000" />
                                                    </g>
                                                </svg>
                                            </span>
                                            <span class="pulse-ring">23</span>
                                        </div>
                                        <span id="notificationCountSpan" class="badge badge-danger" style="margin-top: -35px;margin-left: 25px;position: absolute;color: white;"></span>
                                    </div>


                                    <div id="notificationDropdown" class="dropdown-menu p-0 m-0 dropdown-menu-right dropdown-menu-anim-up dropdown-menu-lg" style="margin-left: -250px !important">

                                        <div id="notificationsTab">
                                            <div class="d-flex flex-column pt-12 bgi-size-cover bgi-no-repeat rounded-top" style="background-image: url(@Url.Content("~/Backend/assets/media/misc/bg-1.jpg"))">

                                                <h4 class="d-flex flex-center rounded-top">
                                                    <span class="text-white">User Notifications</span>
                                                    <span class="btn btn-text btn-success btn-sm font-weight-bold btn-font-md ml-2 notificationCount"></span>
                                                </h4>

                                            </div>


                                            <div class="tab-content">

                                                <div class="tab-pane active show p-8" id="topbar_notifications_notifications" role="tabpanel">

                                                    <div class="scroll pr-7 mr-n7" style="display: block" data-scroll="true" data-height="300" data-mobile-height="200" id="notificationList">


                                                    </div>
                                                    @*<div class="d-flex flex-center pt-7"><a href="#" class="btn btn-light-primedicalRecordy font-weight-bold text-center">See All</a></div>*@

                                                </div>




                                                <div class="tab-pane" id="topbar_notifications_logs" role="tabpanel">
                                                    <div class="d-flex flex-center text-center text-muted min-h-200px">
                                                        All caught up!
                                                        <br />
                                                        No new notifications.
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <div class="topbar-item">
                                    <div class="btn btn-icon btn-hover-transparent-white w-lg-auto d-flex align-items-center btn-lg px-2" id="kt_quick_user_toggle">
                                        <div class="d-flex flex-column text-right pr-lg-3">
                                            <span class="text-white opacity-50 font-weight-bold font-size-sm d-none d-md-inline">
                                                @if (User != null && User.Identity.IsAuthenticated)
                                                {
                                                    var name = ((ClaimsIdentity)User.Identity).FindFirst("FullName");
                                                    if (name != null)
                                                    {
                                                        @Html.Raw(name.Value)
                                                    }
                                                }
                                            </span>
                                            <span class="text-white font-weight-bolder font-size-sm d-none d-md-inline">
                                                @if (User.IsSysAdmin())
                                                {
                                                    @Html.Raw("System Administrator")
                                                }
                                                else
                                                {
                                                    @Html.Raw("User")
                                                }
                                            </span>
                                        </div>
                                        <span class="symbol symbol-35">
                                            <span class="symbol-label font-size-h5 font-weight-bold text-white bg-white-o-30">
                                                @if (User != null && User.Identity.IsAuthenticated)
                                                {
                                                    var name = ((ClaimsIdentity)User.Identity).FindFirst("FullName");
                                                    if (name != null)
                                                    {
                                                        @Html.Raw(name.Value.Take(1).FirstOrDefault())
                                                    }
                                                }
                                            </span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="header-bottom">
                        <div class=" container ">
                            <div class="header-navs header-navs-left" id="kt_header_navs">
                                <ul class="header-tabs p-5 p-lg-0 d-flex d-lg-none nav nav-bold nav-tabs" role="tablist"></ul>
                                <div class="tab-content">
                                    <div class="tab-pane py-5 p-lg-0 show @((controller == "Lead") ? "active" : "")" id="kt_header_tab_1">
                                        <div id="kt_header_menu" class="header-menu header-menu-mobile  header-menu-layout-default ">
                                            <ul class="menu-nav">
                                                <li class="menu-item @((controller == "Lead" && action == "Index") ? "menu-item-active" : "")" aria-haspopup="true"><a href="@Url.RouteUrl("Dashboard")" class="menu-link "><span class="menu-text">Dashboard</span></a></li>
                                            </ul>
                                        </div>
                                    </div>

                                    @if ((User.Identity.IsAuthenticated && User.HasPermission("User-Users")) || User.IsSysAdmin())
                                    {
                                        <div class="tab-pane p-5 p-lg-0 justify-content-between @((controller == "Role") || controller == "User" || controller == "Permission" ? "active" : "")" id="kt_header_tab_2">
                                            <div class="header-menu header-menu-mobile  header-menu-layout-default ">
                                                <ul class="menu-nav">
                                                    <li class="menu-item  menu-item @((controller == "User" && action == "Users") ? "menu-item-active" : "")" aria-haspopup="true">
                                                        <a href="@Url.Action("Users","User")" class="menu-link "><span class="menu-text">Users</span></a>
                                                    </li>
                                                    <li class="menu-item  menu-item @((controller == "Role" && action == "RoleIndex") ? "menu-item-active" : "")" aria-haspopup="true">
                                                        <a href="@Url.Action("RoleIndex","Role")" class="menu-link "><span class="menu-text">Roles</span></a>
                                                    </li>
                                                    <li class="menu-item  menu-item @((controller == "Permission" && action == "Index") ? "menu-item-active" : "")" aria-haspopup="true">
                                                        <a href="@Url.Action("Index","Permission")" class="menu-link "><span class="menu-text">Permission</span></a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="content  d-flex flex-column flex-column-fluid" id="kt_content">

                    @RenderBody()

                </div>


                <div class="footer bg-white py-4 d-flex flex-lg-column " id="kt_footer">
                    <div class=" container  d-flex flex-column flex-md-row align-items-center justify-content-between">
                        <div class="text-dark order-2 order-md-1">
                            <span class="text-muted font-weight-bold mr-2">2020&copy;</span>
                            <a href="https://www.eazisols.com" target="_blank" class="text-dark-75 text-hover-primedicalRecordy">Eazisols</a>
                        </div>
                        <div class="nav nav-dark order-1 order-md-2">
                            <a href="@Url.RouteUrl("About")" target="_blank" class="nav-link pr-3 pl-0">About</a>
                            <a href="@Url.RouteUrl("Contact")" target="_blank" class="nav-link pl-3 pr-0">Contact</a>
                        </div>
                    </div>
                </div>

            </div>

        </div>

    </div>


    <div id="kt_quick_user" class="offcanvas offcanvas-right p-10">

        <div class="offcanvas-header d-flex align-items-center justify-content-between pb-5">
            <h3 class="font-weight-bold m-0">
                User Profile
                @*<small class="text-muted font-size-sm ml-2">12 messages</small>*@
            </h3>
            <a href="#" class="btn btn-xs btn-icon btn-light btn-hover-primedicalRecordy" id="kt_quick_user_close">
                <i class="ki ki-close icon-xs text-muted"></i>
            </a>
        </div>


        <div class="offcanvas-content pr-5 mr-n5">

            <div class="d-flex align-items-center mt-5">
                <div class="symbol symbol-100 mr-5">
                    <div class="symbol-label" style="background-image:url('assets/media/users/300_21.jpg')"></div>
                    <i class="symbol-badge bg-success"></i>
                </div>
                <div class="d-flex flex-column">
                    <a href="#" class="font-weight-bold font-size-h5 text-dark-75 text-hover-primedicalRecordy">
                        @if (User != null && User.Identity.IsAuthenticated)
                        {
                            var name = ((ClaimsIdentity)User.Identity).FindFirst("FullName");
                            if (name != null)
                            {
                                @Html.Raw(name.Value)
                            }
                        }
                    </a>
                    <div class="text-muted mt-1">
                        @if (User.IsSysAdmin())
                        {
                            @Html.Raw("System Administrator")
                        }
                        else
                        {
                            @Html.Raw("User")
                        }
                    </div>
